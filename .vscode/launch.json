    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": { "CUDA_VISIBLE_DEVICES": "3"}, 
            "args": ["--project", "apple_h1",
                    "--compound_coef", "0",
                    "--batch_size", "2",
                    "--lr", "1e-3", 
                    "--load_weights", "weights/efficientdet-d0.pth", 
                    "--num_epochs", "5", 
                    "--es_patience", "300",
                    "--use_seed", "True", 
                    "--seed_values", "42",
                    "--policy", "stac",
                    "--debug", "True"]
        }
    ]
}